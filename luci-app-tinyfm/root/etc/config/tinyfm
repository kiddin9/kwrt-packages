config tinyfm 'auth'
	option enable '1'
	option pilihan 'mihomo'
	option username 'admin'
	option password '$2y$10$mNORLB8mf7am3ywMYSTUOehQhIUSDEe5Yes/4NRXUgsPmE0hQxNBS'
	option readonly_users 'user'

config tinyfm 'symlink'
	option rootfs '/'
	option openclash '/etc/openclash'
	option mihomo '/etc/mihomo'
	option script_path '/usr/bin/tinyfm.sh'
	option pilihan 'rootfs'